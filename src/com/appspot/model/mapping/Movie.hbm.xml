<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- generator:mapping -->
    <class name="com.appspot.model.entity.Movie" table="movie">
        <id name="id" column="id">
            <generator class="native">
                <param name="sequence">movie_id_seq</param>
            </generator>
        </id>
        <property name="date" column="date" not-null="true" type="date" />
        <property name="title" column="title" not-null="true" type="string" length="100" />
        <property name="genre" column="genre" not-null="true" type="string" length="30" />
        <property name="url" column="url" not-null="true" type="string" length="500" />
        <property name="catalogNo" column="catalogNo" not-null="true" type="string" length="300" />
        <property name="upc" column="upc" not-null="true" type="string" length="16" />
    </class>

</hibernate-mapping>
